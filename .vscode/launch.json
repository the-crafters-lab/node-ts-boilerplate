    "version": "0.2.0",
    "configurations": [
      {
        "type": "node",
        "request": "attach",
        "name": "Attach Process",
        "processId": "${command:PickProcess}",
        "restart": true,
        "protocol": "inspector",
        "skipFiles": [
          "<node_internals>/**/*.js",
          "node_modules/**/*.js",
          "async_hooks.js",
          "inspector_async_hook.js",
          "pre_execution.js",
          "domain.js",
          "task_queues.js",
          "*.js",
          "structured-stack",
        ]
      }
    ]
  }